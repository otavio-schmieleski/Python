<View_inicial>
    FloatLayout:
        Label:
            text:'CONTROLE DE IMOBILIZADOS'
            size_hint: .5,.5
            bold: True
            font_size: self.width * 0.1
            pos_hint: {"center_x": .5, "y": .70}
            border: 25
        TextInput:
            id: line_nome
            size_hint: .6,.06
            pos_hint: {"center_x": .5, "y": .65}
            multilane: False
            hint_text: "Informe seu Nome de Usuário"
            bold: True
            halign: 'center' 
            background_normal:"rounded_input.png"
            background_active: "rounded_input.png"
            background_color: 1,1,1,1
            canvas.before:
                Color:
                    rgba: 1, 1, 1, 1
                RoundedRectangle:
                    pos: self.pos
                    size: self.size
                    radius: [20, 20] 
                Color:
                    rgba: 0, 0, 0, 1
            font_size: self.width * 0.035
       
        TextInput:
            id: line_senha
            size_hint: .6,.06
            pos_hint: {"center_x": .5, "y": .50}
            multilane: False
            hint_text: "informe sua Senha"
            bold: True
            halign: 'center' 
            background_normal:"rounded_input.png"
            background_active: "rounded_input.png"
            background_color: 1,1,1,1
            font_size: self.width * 0.035
            canvas.before:
                Color:
                    rgba: 1, 1, 1, 1
                RoundedRectangle:
                    pos: self.pos
                    size: self.size
                    radius: [20, 20] 
                Color:
                    rgba: 0, 0, 0, 1

        Button:
            id: btn_inicial
            text: 'ENTRAR'
            size_hint: .18, .08
            font_size: self.width * 0.2
            pos_hint: {"center_x": .5, "y": .25}
            halign: 'center'
            background_normal:""
            background_color: 0,0,0,0
            canvas.before:
                Color:
                    rgba: 0.3333, 0.6667, 0, 1
                RoundedRectangle:
                    pos: self.pos
                    size: self.size
                    radius: [20, 20, 20, 20]

